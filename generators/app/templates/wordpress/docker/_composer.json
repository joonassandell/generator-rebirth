{
  "name": "<%= dir %>/wp",
  "description" : "<%= appDescription %>",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }, {
      "type": "composer",
      "url": "https://packagist.org"
    }<% if (pluginWPMLuserID) { %>, {
      "type": "package",
      "package": {
        "name": "wpml/sitepress-multilingual-cms",
        "version": "3.8.4",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6088&user_id=<%= pluginWPMLuserID %>&subscription_key=<%= pluginWPMLkey %>"
        }
      }
    }, {
      "type": "package",
      "package": {
        "name": "wpml/wpml-string-translation",
        "version": "2.6.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6092&user_id=<%= pluginWPMLuserID %>&subscription_key=<%= pluginWPMLkey %>"
        }
      }
    }, {
      "type": "package",
      "package": {
        "name": "wpml/wpml-translation-management",
        "version": "2.4.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6094&user_id=<%= pluginWPMLuserID %>&subscription_key=<%= pluginWPMLkey %>"
        }
      }
    }<% } if (pluginACFkey) { %>, {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.5.7",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=<%= pluginACFkey %>"
        }
      }
    }<% } %>
  ],
  "require": {
    "johnpbloch/wordpress": "^4.9.1",
    "wpackagist-plugin/w3-total-cache": "^0.9.6",
    "wpackagist-plugin/contact-form-7": "^4.9.2",
    "wpackagist-plugin/flamingo": "^1.6",
    "wpackagist-plugin/duplicate-post": "^3.2.1",
    "wpackagist-plugin/simple-custom-post-order": "^2.3.2",
    "wpackagist-plugin/timber-library": "1.5.2",
    "wpackagist-plugin/wordpress-seo": "^5.9.3"<% if (pluginWPMLuserID) { %>,
    "wpml/sitepress-multilingual-cms": "^3.8.4",
    "wpml/wpml-string-translation": "^2.6.3",
    "wpml/wpml-translation-management": "^2.4.3"<% } if (pluginACFkey) { %>,
    "advanced-custom-fields/advanced-custom-fields-pro": "^5.5.7"<% } %>
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
