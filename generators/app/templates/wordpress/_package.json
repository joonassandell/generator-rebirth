{
  "name": "<%= appNameDasherize %>",
  "version": "1.0.0",
  "author": "<%= appAuthor %>",
  "homepage": "<%= appURL %>",
  "description": "<%= appDescription %>",
  "dependencies": {
    "normalize.css": "^7.0.0",
    "core-js": "^3.2.1"
  },
  "main": "gulpfile.js",
  "license": "MIT",
  "repository": "<%= appRepositoryURL %>",
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "dev": "gulp dev",
    "deploy": "shipit production deploy",
    "rollback": "shipit production rollback",
    "start": "npm run dev",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babelify": "^10.0.0",
    "browser-sync": "^2.23.2",
    "browserify": "^16.2.3",
    "dotenv": "^6.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "6.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-combine-mq": "^0.4.0",
    "gulp-concat": "^2.5.2",
    "gulp-cssnano": "^2.1.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^8.1.1",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^4.0.1",
    "gulp-streamify": "1.0.2",
    "gulp-uglify-es": "^1.0.4",
    "gulp-util": "~3.0.1",
    "husky": "^0.14.3",
    "install": "^0.12.1",
    "lint-staged": "^7.2.2",
    "node-notifier": "^5.1.2",
    "prettier": "^1.14.3",
    "pretty-hrtime": "^1.0.0",
    "rimraf": "^2.5.0",
    "sass-mq": "^5.0.0",
    "shipit-cli": "^4.1.1",
    "shipit-deploy": "^4.1.1",
    "through2": "^2.0.3",
    "vinyl-source-stream": "~2.0.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "entry",
                "corejs": {
                  "version": 3
                }
              }
            ]
          ]
        }
      ]
    ]
  },
  "engines": {
    "node": ">=9.10.0"
  }
}
