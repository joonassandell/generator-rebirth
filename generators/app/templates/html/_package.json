{
  "name": "<%= appNameDasherize %>",
  "version": "1.0.0",
  "author": "<%= appAuthor %>",
  "homepage": "<%= appURL %>",
  "description": "<%= appDescription %>",
  "dependencies": {
    "core-js": "^3.2.1",
    "handlebars-helpers": "^0.6.1",
    "normalize.css": "^8.0.1"
  },
  "main": "assemblefile.js",
  "repository": "git@bitbucket.org:<%= appAuthorDasherize %>/<%= dir %>.git",
  "scripts": {
    "build": "NODE_ENV=production assemble build",
    "deploy": "npm run build && dploy production",
    "dist": "npm run build && assemble server",
    "start": "npm run dist",
    "watch": "assemble watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "ansi-colors": "^4.1.1",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "assemble": "^0.24.3",
    "babelify": "^10.0.0",
    "base-watch": "^0.1.3",
    "browser-sync": "^2.13.0",
    "browserify": "^16.2.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.1.0",
    "dploy": "^1.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "fancy-log": "^1.3.3",
    "gulp": "~4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-changed": "^4.0.2",
    "gulp-combine-mq": "^0.4.0",
    "gulp-concat": "^2.5.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^4.0.2",
    "gulp-streamify": "1.0.2",
    "gulp-uglify-es": "^1.0.4",
    "husky": "^3.0.8",
    "install": "^0.13.0",
    "lint-staged": "^9.4.1",
    "node-notifier": "^6.0.0",
    "prettier": "^1.16.4",
    "pretty-hrtime": "^1.0.0",
    "regenerator-runtime": "^0.13.3",
    "relative-dest": "^0.1.0",
    "rimraf": "^3.0.0",
    "sass-mq": "^5.0.1",
    "through2": "^3.0.1",
    "vinyl-source-stream": "~2.0.0",
    "watchify": "^3.2.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "entry",
                "corejs": {
                  "version": "3"
                }
              }
            ]
          ]
        }
      ]
    ]
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
