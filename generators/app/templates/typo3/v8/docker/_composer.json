{
  "name": "<%= dir %>/typo3-docker",
  "description" : "<%= appNameHumanize %> TYPO3 Development Environment",
  "homepage": "<%= appURL %>",
  "version": "1.0.0",
  "config": {
    "vendor-dir": "Packages/Libraries"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org"
    }
  ],
  "require": {
    "typo3/cms": "^8.7.8",
    "helhum/typo3-console": "^4.9.2",
    "fluidtypo3/flux": "^8.2.1",
    "fluidtypo3/fluidpages": "dev-development",
    "fluidtypo3/fluidcontent": "^5.2.0",
    "fluidtypo3/vhs": "^4.3.2"
  },
  "require-dev": {
    "nimut/testing-framework": "^2.0.0"
  }
}
